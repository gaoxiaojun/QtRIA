<?xml version="1.0"?>
<Installer>
    <Name>QForex</Name>
    <Version>{version}</Version>
    <Title>QForex {version}</Title>
    <Publisher>QForex Project</Publisher>
    <ProductUrl>http://www.dailypips.org</ProductUrl>

    <InstallerWindowIcon>logo.png</InstallerWindowIcon>
    <Watermark>watermark.png</Watermark>
    <UninstallerName>Uninstall QForex</UninstallerName>
    <!-- @homeDir@ and @rootDir@ are some of the supported vars -->
    <TargetDir>@homeDir@/Applications/QForex {version}</TargetDir>
    <AllowSpaceInPath>true</AllowSpaceInPath>
</Installer>
